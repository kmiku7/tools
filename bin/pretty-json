import sys
import json
import subprocess
import copy
import traceback
from random import randrange


def main(): 
    object = json.load(sys.stdin)
    json.dump(object, sys.stdout, indent=4, sort_keys=True)


if __name__ == "__main__":
    try:    
        main()  
    except: 
        print traceback.print_exc()

